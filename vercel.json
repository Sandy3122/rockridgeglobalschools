{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/bachupally",
      "destination": "/bachupally/index.html"
    },
    {
      "source": "/bachupally/",
      "destination": "/bachupally/index.html"
    },
    {
      "source": "/manikonda",
      "destination": "/manikonda/index.html"
    },
    {
      "source": "/manikonda/",
      "destination": "/manikonda/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
